<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
<schema id="org.gnome.shell.extensions.webling.sizes_and_positions"
	path="/org/gnome/shell/extensions/webling/sizes_and_positions/">
    <key name="win-pos-x" type="i">
      <default>0</default>
      <summary>Window X position</summary>
    </key>
    <key name="win-pos-y" type="i">
      <default>0</default>
      <summary>Window Y position</summary>
    </key>
    <key name="win-size-height" type="i">
      <default>720</default>
      <summary>Height of window</summary>
    </key>
    <key name="win-size-width" type="i">
      <default>1280</default>
      <summary>Width of window</summary>
    </key>
</schema>
<schema id="org.gnome.shell.extensions.webling.toggles"
	path="/org/gnome/shell/extensions/webling/toggles/">
    <key name="always-on-top" type="b">
     <default>true</default>
     <summary>Set Window always on top</summary>
    </key>
</schema>

</schemalist>
